          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features02 authorm5\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Card Icon\" name=\"showIcon\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#ffffff\" name=\"icon\" condition=\"showIcon\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Card Buttons\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Color 1\" value=\"#ffffff\" name=\"color_1\">\n        <input type=\"color\" title=\"Color 2\" value=\"#41798C\" name=\"color_2\">\n        <input type=\"color\" title=\"Color 3\" value=\"#1e6bff\" name=\"color_3\">\n        <input type=\"color\" title=\"Card BG\" value=\"#171827\" name=\"card\">\n        <input type=\"color\" title=\"Card Border\" value=\"#373d52\" name=\"border\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" parallax selected>\n            <input type=\"color\" title=\"Color\" value=\"#232b45\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232b45\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"title-wrapper\" mbr-if=\"showTitle\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\">\n                        All the Books are Part of <br> One Big Story\n                    </h2>\n                </div>\n            </div>\n            <div class=\"col-12\">\n                <div class=\"items-wrapper\">\n                    <div class=\"item features-without-image item-mb\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"card-box\">\n                                <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-preview mobi-mbri\"></span>\n                                </div>\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title, .icon-wrapper, .mbr-section-btn\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\">\n                                    Book №1\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                                    In this gripping psychological thriller, a brilliant but tormented detective races\n                                    against time to catch a serial killer who leaves cryptic messages at crime scenes.\n                                </p>\n                            </div>\n                            <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                                <a class=\"btn btn-secondary-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                    READ NOW\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"item features-without-image item-mb\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"card-box\">\n                                <div class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-layers mobi-mbri\"></span>\n                                </div>\n                                <h4 class=\"item-title mbr-fonts-style\" data-app-selector=\".item-title, .icon-wrapper, .mbr-section-btn\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\">\n                                    Book №2\n                                </h4>\n                                <p class=\"item-text mbr-fonts-style\" data-app-selector=\".item-text\" mbr-theme-style=